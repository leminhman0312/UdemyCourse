#include <stdio.h>

void swap(char *x, char *y){
  char temp;
  temp = *x;
  *x = *y;
  *y = temp;
}



void perm2(char s[], int l, int h){
  int i;

  if (l == h) {
    printf("%s\n", s);
  }

  else {
    for (i = l; i <= h; i++) {
      swap(&s[l],&s[h]);
      perm2(s,l+1,h);
      swap(&s[l],&s[i]);
    }
  }
}

int main(){
  char s[] = "ABC";
  perm2(s,0,2);
  return 0;
}
