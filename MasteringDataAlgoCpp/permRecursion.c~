#include <stdio.h>

void perm(char s[], int k){
  static int A[10] = {0};
  static char Res[10];
  int i;
  if (s[k] == '\0') {
    Res[k] = '\0';		/* marked the last elements in Res to be \0 for string */
    printf("%s\n", Res);
  }
  else {
    for (i = 0; s[i] != '\0'  ; i++) {
      /* check if alphabet available or not */
      if (A[i] == 0) {
	Res[k] = s[i];
	A[i] = 1;		/* marked as 1 alphabet NOT available */
	perm(s,k+1);
	A[i] = 0;		/* marked as 0 for RETURNING call */
      }
    }
  }
}

int main(){
  char s[] = "ABC";
  perm(s,0);
  return 0;
}
